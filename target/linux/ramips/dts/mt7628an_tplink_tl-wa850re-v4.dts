/dts-v1/;

#include "mt7628an.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "tplink,tl-wa850re-v4", "mediatek,mt7628an-soc";
	model = "TP-Link TL-WA850RE v4";

	chosen {
		bootargs = "console=ttyS0,57600n8";
	};

	aliases {
		label-mac-device = &ethernet;

		led-boot = &led_power;
		led-failsafe = &led_power;
		led-running = &led_power;
		led-upgrade = &led_power;
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			gpios = <&gpio1 5 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};

		wps {
			label = "wps";
			gpios = <&gpio1 6 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};
	};

	leds {
		compatible = "gpio-leds";

		led_power: power {
			label = "tl-wa850re-v4:blue:power";
			gpios = <&gpio1 8 GPIO_ACTIVE_LOW>;
		};

		lan {
			label = "tl-wa850re-v4:blue:lan";
			gpios = <&gpio1 7 GPIO_ACTIVE_LOW>;
		};

		wlan {
			label = "tl-wa850re-v4:blue:wlan";
			gpios = <&gpio1 9 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy0tpt";
		};

		range_extender {
			label = "tl-wa850re-v4:blue:RE";
			gpios = <&gpio1 10 GPIO_ACTIVE_LOW>;
		};

		rssi1 {
			label = "tl-wa850re-v4:blue:rssi1";
			gpios = <&gpio1 11 GPIO_ACTIVE_LOW>;
		};

		rssi2 {
			label = "tl-wa850re-v4:blue:rssi2";
			gpios = <&gpio0 5 GPIO_ACTIVE_LOW>;
		};

		rssi3 {
			label = "tl-wa850re-v4:blue:rssi3";
			gpios = <&gpio0 4 GPIO_ACTIVE_LOW>;
		};

		rssi4 {
			label = "tl-wa850re-v4:blue:rssi4";
			gpios = <&gpio1 12 GPIO_ACTIVE_LOW>;
		};

		rssi5 {
			label = "tl-wa850re-v4:blue:rssi5";
			gpios = <&gpio0 11 GPIO_ACTIVE_LOW>;
		};

	};

};


&spi0 {
	status = "okay";

	m25p80@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <10000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "boot";
				reg = <0x0 0x20000>;
				read-only;
			};

			partition@20000 {
				compatible = "tplink,firmware";
				label = "firmware";
				reg = <0x20000 0x3a0000>;
			};

			tpconfig: partition@3c0000 {
				label = "tplink-part-config";
				reg = <0x3c0000 0x10000>;
				read-only;
			};

			partition@3d0000 {
				label = "tplink-user-config";
				reg = <0x3d0000 0x10000>;
				read-only;
			};

			partition@3e0000 {
				label = "tplink-default-config";
				reg = <0x3e0000 0x10000>;
				read-only;
			};

			/* or radio, but mt2628an.dtsi likes factory */
			factory: partition@3f0000 {
				label = "factory";
				reg = <0x3f0000 0x10000>;
				read-only;
			};
		};
	};
};

&ehci {
	status = "disabled";
};

&ohci {
	status = "disabled";
};

&wmac {
	status = "okay";
	mtd-mac-address = <&tpconfig 0x4008>;
	mtd-mac-address-increment = <1>;
};

&ethernet {
	mtd-mac-address = <&tpconfig 0x4008>;
};


&state_default {
	gpio {
		ralink,group = "refclk", "wdt";
		ralink,function = "gpio";
	};
};
